{
  "$schema": "https://aleph.network/schemas/aleph-extension-v1.json",
  "type": "aleph-extension",
  "alephVersion": "1.0.0",
  "capabilities": {
    "skillProvider": true,
    "dsnEnabled": true,
    "semanticDomain": "meta"
  },
  "skills": [
    {
      "name": "get_reputation",
      "description": "Get the reputation score for a node or service",
      "executionLocation": "CLIENT"
    },
    {
      "name": "submit_feedback",
      "description": "Submit feedback for a service interaction",
      "executionLocation": "CLIENT"
    }
  ],
  "permissions": [
    "dsn:register-tool",
    "dsn:publish-observation",
    "store:read",
    "store:write"
  ]
}
