{
  "name": "alephnet-client",
  "version": "0.1.0",
  "description": "AlephNet Resonant Terminal",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "vite --host",
    "dev:electron": "vite",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "start": "electron .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "alephnet",
    "electron",
    "react",
    "gun"
  ],
  "author": "AlephNet Team",
  "license": "MIT",
  "devDependencies": {
    "@electron/rebuild": "^4.0.3",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.24",
    "electron": "^30.0.0",
    "electron-builder": "^24.13.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "ts-jest": "^29.1.2",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vite-plugin-electron": "^0.11.0",
    "vite-plugin-electron-renderer": "^0.14.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.38",
    "@ai-sdk/gateway": "^3.0.40",
    "@ai-sdk/google": "^3.0.22",
    "@ai-sdk/openai": "^3.0.26",
    "@mlc-ai/web-llm": "^0.2.80",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sschepis/alephnet-node": "^1.4.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "ai": "^6.0.77",
    "class-variance-authority": "^0.7.1",
    "clawhub": "^0.5.0",
    "clsx": "^1.0.0",
    "cmdk": "^1.1.1",
    "electron-squirrel-startup": "^1.0.1",
    "eventemitter3": "^5.0.4",
    "flexlayout-react": "^0.8.18",
    "framer-motion": "^10.0.0",
    "gun": "^0.2020.0",
    "katex": "^0.16.28",
    "lucide-react": "^0.563.0",
    "mermaid": "^11.12.2",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "quickjs-emscripten": "^0.31.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "recharts": "^2.0.0",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "robotjs": "^0.6.0",
    "tailwind-merge": "^1.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.0.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.1.0",
    "utf-8-validate": "^5.0.10"
  },
  "overrides": {
    "node-abi": "^3.65.0"
  },
  "build": {
    "appId": "com.alephnet.client",
    "productName": "AlephNet Client",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "mac": {
      "artifactName": "${productName}-${version}-Mac.${ext}"
    },
    "win": {
      "artifactName": "${productName}-${version}-Windows.${ext}"
    },
    "linux": {
      "artifactName": "${productName}-${version}-Linux.${ext}"
    }
  }
}
